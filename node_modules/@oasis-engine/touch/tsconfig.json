{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext", "DOM"],
		"moduleResolution": "Node",
		"declarationDir": "types",
		"declaration": true,
		"skipLibCheck": true,
		"strict": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"emitDeclarationOnly": true
	},
	"include": ["./src"]
}
